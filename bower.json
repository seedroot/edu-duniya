{
  "name": "edu-duniya",
  "dependencies": {
    "angular": "~1.4.8",
    "angular-bootstrap": "~1.0.3",
    "angular-notify": "~2.5.0",
    "angular-ui-router": "~0.2.15",
    "animate.css": "latest",
    "bootstrap": "latest",
    "bootstrap-switch": "latest",
    "Chart.js": "latest",
    "datatables": "latest",
    "font-awesome": "latest",
    "iCheck": "latest",
    "jquery": "latest",
    "matchHeight": "latest",
    "ngstorage": "~0.3.10",
    "noty": "~2.3.7",
    "select2": "latest"
  },
  "private": true,
  "overrides": {
    "jquery": {
      "main": [
        "dist/jquery.min.js"
      ]
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css",
        "fonts/*"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.min.css",
        "fonts/*"
      ]
    },
    "animate.css": {
      "main": [
        "animate.min.css"
      ]
    },
    "bootstrap-switch": {
      "main": [
        "dist/css/bootstrap3/bootstrap-switch.min.css",
        "dist/js/bootstrap-switch.min.js"
      ]
    },
    "iCheck": {
      "main": [
        "skins/flat/_all.css",
        "icheck.min.js"
      ]
    },
    "datatables": {
      "main": [
        "media/css/jquery.dataTables.min.css",
        "media/css/dataTables.bootstrap.css",
        "media/js/jquery.dataTables.min.js",
        "media/js/dataTables.bootstrap.min.js"
      ]
    },
    "ace-builds": {
      "main": [
        "src/ace.js",
        "src/mode-html.js",
        "src/theme-github.js"
      ]
    },
    "Chart.js": {
      "main": [
        "Chart.min.js"
      ]
    },
    "select2": {
      "main": [
        "dist/js/select2.full.min.js",
        "dist/css/select2.min.css"
      ]
    }
  }
}
